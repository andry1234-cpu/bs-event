{
  "rules": {
    "messages": {
      ".read": "auth != null",
      ".write": "auth != null",
      ".indexOn": ["timestamp"]
    },
    "reactions": {
      ".read": "auth != null",
      ".write": "auth != null",
      ".indexOn": ["timestamp"]
    },
    "presence": {
      ".read": "auth != null",
      ".write": "auth != null",
      ".indexOn": ["lastSeen"]
    }
  }
}
